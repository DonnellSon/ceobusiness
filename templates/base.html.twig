<!DOCTYPE html>
<html>

<head>
	<meta charset="UTF-8">
	<title>
		{% block title %}CEO BUSINESS FORUM OF INDIAN OCEAN
		{% endblock %}
	</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet"
		href="https://cdn.jsdelivr.net/npm/@tarekraafat/autocomplete.js@10.2.7/dist/css/autoComplete.min.css">
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

	<link rel="icon" href="{{ asset('assets/img/png/ceo2.png') }}" type="image/x-icon">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css"
		integrity="sha512-SnH5WK+bZxgPHs44uWIX+LLJAJ9/2PkPKZ5QiAj6Ta86w+fsb2TkcmfRyVX3pBnMFcV7oQPJkl9QevSCWr3W6A=="
		crossorigin="anonymous" referrerpolicy="no-referrer" />
	<link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
	<link rel="stylesheet" href="{{ asset('assets/css/media.css') }}">
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"
		integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g=="
		crossorigin="anonymous" referrerpolicy="no-referrer"></script>
	{% block links %}{% endblock %}
	{% block head %}{% endblock %}
	<link rel="stylesheet" href="{{ asset('assets/css/animate.css') }}">
</head>

<body>
	<div class="global-messages">
		{% for err in app.flashes('globalErrors') %}
		<div class="error-label">
			<span class="close-label">&times;</span>
			<p>{{ err }}</p>
		</div>
		{% endfor %}
		{% for s in app.flashes('globalSuccess') %}
		<div class="success-label">
			<span class="close-label">&times;</span>
			<p>{{ s }}</p>
		</div>
		{% endfor %}
	</div>

	<header class="px-lg-0 px-md-0 px-sm-3 px-3">
		<div class="d-flex align-items-center justify-content-between">
			<div class="left">
				<a href="{{ path('home') }}" class="topbar-logo">
					<img src="{{ asset('assets/img/png/ceo2.png') }}" alt="">
				</a>
			</div>
			<div class="menu">
				<div class="btn close-btn">
					<i class="fas fa-times"></i>
				</div>
				<a class="nav-btn" href="{{ path('home') }}#acceuil">{{ 'Home' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#secteur">{{ 'Sectors' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#objectifs">{{ 'Objectives' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#cibles">{{ 'Our targets' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#reportage">{{ 'Coverages' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#startuper">{{ 'Startupper of the Year' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#hote">{{ 'Host' | trans }}</a>
				<a class="nav-btn" href="{{ path('home') }}#pavillon">{{ 'Pavilion' | trans }}</a>

				<a href="{{ path('personal_register') }}" class="register-btn px-3">
					<i class="fa-solid fa-pen-nib"></i>
					{{ 'Register' | trans }}</a>
				{% if app.user %}
				<a href="{{ path('app_logout') }}" class="register-btn px-3">
					<i class="fas fa-sign-out-alt"></i>
				</a>
				{% endif %}
				<div class="dropdown">
					<button class="nav-btn dropdown-toggle lang" type="button" data-bs-toggle="dropdown"
						aria-expanded="false">
						{% if app.request.locale == 'fr' %}
						<img src="{{ asset('assets/img/flag/french.png') }}" style="width:20px; height:20px" alt="">
						<small>fr</small>
						{% elseif app.request.locale == 'en' %}
						<img src="{{ asset('assets/img/flag/united.png') }}" style="width:20px; height:20px" alt="">
						<small>en</small>
						{% endif %}
					</button>
					<ul class="dropdown-menu lang-d-menu">
						{% if app.request.locale == 'fr' %}
						<li>
							<a class="nav-btn dropdown-item" href="{{ path('home', {_locale: 'en'}) }}">
								<img src="{{ asset('assets/img/flag/united.png') }}" style="width:20px; height:20px"
									alt="">
								EN
							</a>
						</li>
						{% elseif app.request.locale == 'en' %}
						<li>
							<a class="nav-btn dropdown-item" href="{{ path('home', {_locale: 'fr'}) }}">
								<img src="{{ asset('assets/img/flag/french.png') }}" style="width:20px; height:20px"
									alt="">
								FR
							</a>
						</li>
						{% endif %}
					</ul>
				</div>

			</div>
			<div class="expand-sidebar-btn">
				<i class="fa fa-bars menu-btn"></i>
			</div>
		</div>
	</header>

	{% block body %}{% endblock %}
	<div class="floatting-right-center">
		<a href="{{ path('personal_register') }}" class="floatting-register-btn animated" data-type="bounceIn"
			data-duration="1" data-delay="0">
			<span>{{ 'Sponsor' | trans }}</span>
		</a>
		<a href="{{ path('personal_register') }}" class="floatting-register-btn animated" data-type="bounceIn"
			data-duration="1" data-delay="0.5">
			<span>{{ 'Register' | trans }}</span>
		</a>
	</div>

	<footer>
		<div class="container top px-4">
			<div class="row">
				<div class="mb-3 newsletter col-lg-3 col-md-6 col-sm-12 col-121">
					<h2>NewsLetter</h2>
					<p class="text-white">{{ 'Stay up to date with our newsletter, exclusive and personalized for you! Enjoy the latest news and exclusive offers now.' | trans }}</p>
					<form method="post" action="{{ path('news_letter') }}"
						class="news-letter-form d-flex align-items-center">
						<input type="texte" placeholder="Votre email" class="form-control" name="newLetterEmail">
						<button class="btn btn-primary">Go</button>
					</form>
				</div>
				<div class="mb-3 latest col-lg-3 col-md-6 col-sm-12 col-121">
					<h2>{{ 'Latest News' | trans }}</h2>
					<ul class="footer-news">
					</ul>
				</div>
				<div class="mb-3 quicklinks col-lg-3 col-md-6 col-sm-12 col-121">
					<h2>Tags</h2>
					<ul class="tags-ul">
						<li>
							<a href="#">#CEO</a>
						</li>
						<li>
							<a href="#">#BusinessForum</a>
						</li>
						<li>
							<a href="#">#IndianOcean</a>
						</li>
						<li>
							<a href="#">#CeoBusinessForumIndianOcean</a>
						</li>
						<li>
							<a href="#">#Investment</a>
						</li>
						<li>
							<a href="#">#Opportunity</a>
						</li>
					</ul>
				</div>
				<div class="mb-3 contact col-lg-3 col-md-6 col-sm-12 col-12">
					<h2>{{ 'Contact us' | trans}}</h2>
					<ul class="info">
						<li class="mb-2">
							<h6>Informations</h6>
							<p class="m-0">
								<a href="#">info@ceobusinessforum.io</a>
							</p>
						</li>
						<li class="mb-2">
							<h6 class="m-0">{{ 'Phone' | trans }}</h6>
							<p>
								<a href="#">+261 37 07 777 07 | +261 38 38 777 72</a>
							</p>
						</li>
						<li class="mb-2">
							<h6 class="m-0">{{ 'Press' | trans }}</h6>
							<p class="m-0">
								<a href="#">press@ceobusinessforum.io</a>
							</p>
						</li>
						<li class="mb-2">
							<h6 class="m-0">{{ 'Sponsor us' | trans }}</h6>
							<p class="m-0">
								<a href="#">sponsor@ceobusinessforum.io</a>
							</p>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<hr>
		<div class="container d-flex align-items-center justify-content-center">
			<div class="copyright">
				<p class="text-align-center">Copyright &copy; 2024 CEO Business Forum and GateNews. All Rights Reserved
				</p>
			</div>
		</div>
	</footer>

	<div class="msg-style">
		<form class="box{{ errors.email is defined or errors.object is defined or errors.message is defined ? ' opened' }}" method="post" action="{{ path('email') }}">
			<div class="top d-flex align-items-center justify-content-between">
				<div class="left d-flex align-items-center gap-10">
					<i class="fa-regular fa-envelope"></i><h3>Envoyer un message</h3>
				</div>
				<div class="right">
					<div role="button" class="close"><i class="fa-solid fa-xmark"></i></div>
				</div>
			</div>
			<div class="body d-flex flex-column">
				<input type="text" name="email" placeholder="Votre email">
				{% if errors.email is defined %}
					<small class="invalid-feedback d-block text-danger">{{ errors.email }}</small>
				{% endif %}
				<input type="text" name="object" placeholder="Objet">
				{% if errors.object is defined %}
					<small class="invalid-feedback d-block text-danger">{{ errors.object }}</small>
				{% endif %}
				<textarea name="message" placeholder="Votre message"></textarea>
				{% if errors.message is defined %}
					<small class="invalid-feedback d-block text-danger">{{ errors.message }}</small>
				{% endif %}
			</div>
			<div class="footer d-flex align-items-center">
				<button class="btn btn-primary"><i class="fa-solid fa-paper-plane"></i></button>
			</div>
		</form>
		<button><i class="fa-solid fa-comment-dots"></i></button>
	</div>

	{# <div class="dropdown mail-sender-dropdown">
		<button class="nav-btn dropdown-toggle lang mail-sender-btn" type="button" data-bs-toggle="dropdown"
			aria-expanded="false">
			<i class="fa-regular fa-comment-dots"></i>
		</button>
		<div class="dropdown-menu">
			<form class="send-mail-dialog" action="" method="post">
				<div class="top d-flex align-items-center justify-content-between">
					<div class="left">
						<h2>Envoyer un email</h2>
					</div>
					<div class="right">

					</div>
				</div>
				<div class="body">

				</div>
				<div class="bottom">
					<button class="btn btn-primary">Envoyer</button>
				</div>
			</form>
		</div>
	</div> #}
</body>

</html>
</div>
